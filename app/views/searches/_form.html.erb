<% if user_signed_in? %>
  <div class="search_form d-flex justify-content-center mt-1 mb-1">
    <%= form_with url: search_path, local: true, method: :get do |f| %>
      <%= f.text_field :word %>
      <%= f.select :model, options_for_select([["User"], ["Book"]]) %>
     <%= f.submit "検索" %>
    <% end %>
  </div>
<% end %>